body {
	margin: 0;
	padding: 0;
	background-image: url("../../../images/body_bg.png");
	background-position: -175px 0px;
	background-repeat: no-repeat;
	background-color: #bde1f5;
}

#content_main {
	width: 980px;
	margin: 0 auto;
	margin-top: 60px;
	text-align: left;
}

#content_div {
	background-image: url("../../../images/repeat_bg_y.png");
	float: left;
}

#content_div_left {
	float: left;
	width: 150px;
	padding-top: 10px;
	min-height: 400px;
}

#content_div_center {
	float: left;
	width: 600px;
}

#content_div_right {
	float: left;
	width: 230px;
}

.left_menu a {
	text-decoration: none;
	color: black;
}

.left_menu a:VISITED {
	color: black;
}

.left_menu_span {
	display: block;
	font-size: 14px;
	width: 150px;
	height: 35px;
}

.left_icon {
	display: block;
	float: left;
	width: 25px;
	height: 25px;
	margin-top: 5px;
	margin-left: 20px;
	background-image: url("../../../images/icon.png");
}

#left_icon_index {
	background-position: -0px -20px;
}

#left_icon_message {
	background-position: -0px -245px;
}

#left_icon_collection {
	background-position: -0px -120px;
}

#left_icon_sendtome {
	background-position: -0px -145px;
}

#left_icon_friend {
	background-position: -0px -220px;
}

.left_mf {
	display: block;
	float: left;
	padding-top: 7px;
	margin-top: 5px;
	margin-left: 5px;
}

.left_menu_cut {
	margin-top: 12px;
	width: 150px;
	height: 9px;
	font-size: 0;
	border-top: solid rgb(217, 217, 217) 1px;
}

.left_menu_cut span {
	font-size: 12px;
	font-weight: bold;
	color: rgb(128, 128, 128);
}

.left_menu_cut_text {
	position: relative;
	top: -7px;
	left: 20px;
	float: left;
}

.left_menu_hidebtn {
	display: none;
	background-image: url("../../../images/left_manager_show.png");
	width: 47px;
	height: 22px;
	float: left;
	position: relative;
	top: -12px;
	left: 65px;
}

#left_grouping_menu {
	float: left;
	position: absolute;
	top: 295px;
}

#left_grouping_menu span {
	font-size: 12px;
}

#left_grouping_menu .left_icon {
	background-position: 0 -173px;
}

#left_grouping_menu .left_menu_span {
	height: 30px;
}

#left_grouping_menu .left_mf {
	padding-top: 5px;
	margin-top: 3px;
}

.grouping_setting {
	display: none;
	float: right;
	width: 13px;
	height: 13px;
	margin-top: 10px;
	margin-right: 15px;
	background-image: url("../../../images/icon.png");
	background-position: -75px -50px;
}

/* 隐藏div */
#left_message_show {
	display: none;
	height: 140px;
	width: 150px;
	background-color: red;
}

/* 正文中 */
#center_weibo {
	clear: both;
	margin-top: 30px;
	margin-left: 20px;
	width: 560px;
	height: 170px;
}

#center_title {
	display: block;
	float: left;
	width: 220px;
	height: 25px;
	background-image: url("../../../images/all_iconbtn.png");
	background-position: 0 -276px;
}

#center_hot {
	display:none;
	float: right;
	font-size: 12px;
	color: rgb(128,128,128);
	margin-top: -5px;
}

#conter_weibo_wn{
	font-family: "Georgia";
	font-size: 20px;
	font-weight: bold;
}

#center_weibo_content {
	margin-top: 10px;
	width: 560px;
	height: 90px;
	overflow: auto;
	padding-top: 10px;
	padding-left: 4px;
	resize: none;
	border: solid rgb(204, 204, 204) 1px;
	font-size: 13px;
	font-family: "微软雅黑";
}

#center_weibo_corner {
	position: relative;
	display: block;
	width: 15px;
	height: 20px;
	left:559px;
	top:-70px;
	background-image: url("../../../images/send_weibo.png");
	background-position: 0 -120px;
}

#center_send {
	width: 560px;
	height: 30px;
	margin-top: -10px;
}

#center_face,#center_uppic,#center_mood {
	display: block;
	width: 20px;
	height: 20px;
	float: left;
	margin-right: 18px;
	background-image: url("../../../images/webim_icon.png");
}

#center_face {
	background-position: -180px 0;
}

#center_uppic {
	background-position: -180px -25px;
}

#center_mood {
	background-position: -180px -150px;
}

#center_send_public {
	float:left;
}

#center_sendweibo {
	display: block;
	float: right;
	width: 82px;
	height: 30px;
	position:relative;
	margin-top:-5px;
	background-image:url("../../../images/send_weibo.png");
	background-position:-50px -131px;
}

/* 正文右 */
#right_template {
	float: right;
	margin-bottom: -20px;
	display: block;
	width: 20px;
	height: 20px;
	background-image: url("../../../images/send_weibo.png");
	background-position: -280px 0;
}

#right_personinfo {
	margin-top: 20px;
	width: 230px;
	height: 165px;
}

#right_headphoto {
	float: left;
	width: 81px;
	height: 81px;
	margin-left: 20px;
	margin-right: 10px;
	background-color: red;
}

#right_userdiv a {
	color: black;
	text-decoration: none;
	font-family: "微软雅黑";
	font-size: 12px;
	font-weight: bold;
}

#right_user {
	float: left;
	display: block;
	width: 100px;
	margin-top: 5px;
	height: 20px;
}

#right_level {
	float: left;
	width: 50px;
	height: 20px;
}

#right_attention,#right_fans,#right_weibo {
	height: 30px;
	float: left;
	margin-top: 20px;
}

#right_attention {
	margin-left: 20px;
	margin-right: 5px;
}

#right_attention span {
	display: block;
}

#right_fans {
	border-left: solid rgb(230, 230, 230) 1px;
	border-right: solid rgb(230, 230, 230) 1px;
	padding-left: 6px;
	padding-right: 6px;
}

#right_fans span {
	display: block;
}

#right_weibo {
	padding-left: 6px;
	padding-right: 6px;
}

#right_weibo span {
	display: block;
}

.right_text1 {
	font-size: 20px;
	font-family: "微软雅黑";
}

.right_text2 {
	margin-top: 1px;
	font-size: 12px;
}