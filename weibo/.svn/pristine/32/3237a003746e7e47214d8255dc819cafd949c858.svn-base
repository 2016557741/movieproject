package www.csdn.project.domain;

import java.io.Serializable;
import java.util.Date;
/**
 * 
 * @author 曹建波	2013-5-2 下午3:24:00
 *
 */
public class Relation implements Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	private Integer id;
	private Date time;
	private String note;
	private String groupName;
	private Users tusers;
	private Users byusers;
	
	
	
	public Relation() {
		super();
	}
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public Date getTime() {
		return time;
	}
	public void setTime(Date time) {
		this.time = time;
	}
	public String getNote() {
		return note;
	}
	public void setNote(String note) {
		this.note = note;
	}
	public String getGroupName() {
		return groupName;
	}
	public void setGroupName(String groupName) {
		this.groupName = groupName;
	}
	
	public Users getTusers() {
		return tusers;
	}
	public void setTusers(Users tusers) {
		this.tusers = tusers;
	}
	public Users getByusers() {
		return byusers;
	}
	public void setByusers(Users byusers) {
		this.byusers = byusers;
	}
	
	
}
