body {
	margin: 0;
	padding: 0;
	background-image: url("../../../images/body_bg.png");
	background-position: -175px 0px;
	background-repeat: no-repeat;
	background-color: #bde1f5;
}

#content_main {
	width: 980px;
	height: 600px;
	margin: 0 auto;
	margin-top: 60px;
	text-align: left;
	background-image: url("../../../images/repeat_bg_y2.png");
}

#content_div_left {
	float: left;
	width: 150px;
	height: 600px;
	/*background-color: pink;*/
}
#div_left_titile{
	width: 150px;
	height: 52px;
	background-image: url("../../../images/attention_title.png");
}
.div_left_div{
	width: 150px;
	height: 35px;
}

#div_left_attentions_group{
	width: 150px;
}
.group_div{
	width: 147px;
}
.group_text{
	float: right;
	width: 100px;
	line-height: 30px;
	font-size: 13px;
}

.content_div_left_div a{
	font-family: "微软雅黑";
	font-size: 15px;
	color: #000000;
	text-decoration: none;
}
.content_div_left_div a:hover{
	font-family: "微软雅黑";
	font-size: 15px;
	color: #000000;
	text-decoration: none;
}

#left_attentions_ico,#left_secret_ico,#left_fance_ico,#left_finduser_ico,#left_guess_ico{
	float: left;
	margin-left: 20px;
	margin-top: 7.5px;
	width: 22px;
	height: 20px;
}

#left_attentions_ico{
	background-image: url("../../../images/attention_icon.png");
	background-position: 0px 0px;
}
#left_secret_ico{
	background-image: url("../../../images/attention_icon.png");
	background-position: 0px -48px;
}
#left_fance_ico{
	background-image: url("../../../images/attention_icon.png");
	background-position: 0px -24px;
}
#left_finduser_ico{
	background-image: url("../../../images/attention_icon.png");
	background-position: 0px -100px;
}
#left_guess_ico{
	background-image: url("../../../images/attention_icon.png");
	background-position: 0px -124px;
}


.left_text{
	float: left;
	margin-left: 10px;
	margin-top: 7.5px;
	width: 90px;
	height: 25px;
	font-family: "微软雅黑";
	font-size: 15px;
}

/*
	右边
*/

#content_div_right{
	float: left;
	width: 830px;
	background-color: #fafafa;
}
#right_top_titile{
	float: left;
	margin-left: 20px;
	margin-top: 20px;
	width: 790px;
	height: 30px;
	border-bottom:#EB192D solid 2px;
	font-size: 15px;
	font-weight: bold;
	color:#333333;
	line-height: 30px;
}
#right_div_button{
	float: left;
	margin-left: 20px;
	margin-top: 25px;
	width: 790px;
	height: 35px;
	background-color: #F1F1F1;
}
#right_div_user{
	float: left;
	margin-left: 5px;
	width: 820px;
	height: 500px;
}
.attention_userinfo{
	width: 250px;
	height: 100px;
	background-color: #fafafa;
	float: left;
	margin-left: 16px;
	margin-top: 16px;
	border: #E6E6E6 solid 1px;
	color: #333;
	filter: progid:DXImageTransform.Microsoft.Shadow(color=#F2F2F2,direction=120,strength=4);/*ie*/
	-moz-box-shadow: 2px 2px 2px #F2F2F2;/*firefox*/
	-webkit-box-shadow: 2px 2px 2px #F2F2F2;/*safari或chrome*/
	box-shadow: 2px 2px 2px #F2F2F2;/*opera或ie9*/
}
.userinfo_img{
	width: 50px;
	height: 50px;
	float: left;
	margin-top: 10px;
	margin-left: 10px;
}
.userinfo_info{
	width: 120px;
	height: 50px;
	float: left;
	margin-top: 10px;
	margin-left: 10px;
}
.userinfo_info table{
	font-family: "微软雅黑";
	font-size: 13px;
	line-height: 13px;
}
.userinfo_intro{
	width: 220px;
	height: 20px;
	float: left;
	font-family: "微软雅黑";
	font-size: 13px;
	line-height: 13px;
	margin-top: 10px;
	margin-left: 10px;
	color:#808080;
}
.userinfo_operate{
	width: 220px;
	height: 20px;
	float: left;
	margin-top: 10px;
	margin-left: 10px;
}
.private_ico{
	float: left;
	width: 10px;
	height: 10px;
	margin-top: 2px;
	line-height: 13px;
	background-image: url("../../../images/webim_icon.png");
	background-position: 0px 0px;
}
.operate_div{
	float: left;
	margin-left:5px;
	height: 13px;
	font-family: "微软雅黑";
	font-size: 13px;
	line-height: 13px;
}
.operate_div span{
	
	font-size: 12px;
	color: #E6E6E6;
}
.select_gif{
	width: 29px;
	height: 29px;
	float: right;
}
.user_id{
	float: right;
}
a {
	vertical-align: bottom;
	font-family: "微软雅黑";
	font-size: 12px;
	color: #0a8cd2;
	text-decoration: none;
}
a:hover {
	vertical-align: bottom;
	font-family: "微软雅黑";
	font-size: 12px;
	color: #0a8cd2;
	text-decoration: underline;
}