<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="www.csdn.project.domain">
	<class name="Family" table="FAMILY" schema="graduation">
         <id name="id" type="java.lang.Integer">
            <column name="FAMILY_ID" />
            <generator class="native" />
        </id>
		<property name="name" type="string" column="FAMILY_NAME" />
		<property name="comment" type="string" column="FAMILY_COMMENT" />
		<property name="userinfoId" type="integer" column="USERINFO_ID" />
		<property name="createDate" type="timestamp" column="CREATE_DATE" />
 		 
 	</class>

</hibernate-mapping>
